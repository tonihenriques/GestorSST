@if (!string.IsNullOrWhiteSpace(TempData["FeedbackSuccess"]?.ToString()))
{
    <div class="alert alert-success alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <h4><i class="icon fa fa-smile-o"></i> Tudo certo!</h4>
        @Html.Raw(TempData["FeedbackSuccess"].ToString())
    </div>
}

@if (!string.IsNullOrWhiteSpace(TempData["FeedbackError"]?.ToString()))
{
    <div class="alert alert-error alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <h4><i class="icon fa fa-frown-o"></i> Ops! Ocorreu algum problema.</h4>
        @TempData["FeedbackError"].ToString()
    </div>
}

@if (!string.IsNullOrWhiteSpace(TempData["FeedbackWarning"]?.ToString()))
{
    <div class="alert alert-warning alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <h4><i class="icon fa fa-meh-o"></i> Ops! Algum detalhe precisa de sua atenção.</h4>
        @TempData["FeedbackWarning"].ToString()
    </div>
}

@if (!string.IsNullOrWhiteSpace(TempData["FeedbackInformation"]?.ToString()))
{
    <div class="alert alert-info alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <h4><i class="icon fa fa-info-circle"></i> A título de informação:</h4>
        @TempData["FeedbackInformation"].ToString()
    </div>
}